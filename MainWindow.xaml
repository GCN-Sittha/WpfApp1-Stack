<Window x:Class="WpfApp1_Stack.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:WpfApp1_Stack"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <Grid>
        <TextBox x:Name="txtData" HorizontalAlignment="Left" Height="89" Margin="317,57,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="159"/>
        <Button x:Name="btnAdd" Content="Add" HorizontalAlignment="Left" Height="49" Margin="317,183,0,0" VerticalAlignment="Top" Width="159" Click="btnAdd_Click"/>
        <Button x:Name="btnDelete" Content="Delete" HorizontalAlignment="Left" Height="58" Margin="317,278,0,0" VerticalAlignment="Top" Width="159" Click="btnDelete_Click"/>

    </Grid>
</Window>
